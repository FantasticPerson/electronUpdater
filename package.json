{
  "name": "win7",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . win7 --platform=win32 --arch=ia32 --overwrite=true --ignore=node_modules/electron-* --ignore=node_modules/.bin --ignore=.git --ignore=dist --prune=true",
    "package": "asar pack True.test-win32-x64/resources/app True.test-win32-x64/resources/app.asar && rm -rf True.test-win32-x64/resources/app"
  },
  "devDependencies": {
    "electron-prebuilt": "0.31.2"
  },
  "dependencies": {
    "extract-zip": "^1.6.5"
  }
}
